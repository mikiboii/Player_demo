<UserControl x:Class="Player_demo.VideoPlayerControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Player_demo"
             xmlns:wf="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
             
             xmlns:winforms="clr-namespace:System.Windows.Forms.Integration;assembly=WindowsFormsIntegration"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <WindowsFormsHost x:Name="windowsFormsHost">
            <wf:Form x:Name="winFormsForm" 
                     FormBorderStyle="None" 
                     ShowInTaskbar="False"
                     TopLevel="False"/>
        </WindowsFormsHost>
    </Grid>
</UserControl>
